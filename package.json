{
  "name": "vite-scaffold",
  "version": "0.0.0",
  "license": "private",
  "scripts": {
    "build": "tsc && vite build",
    "serve": "vite preview",
    "css": "postcss ./src/Styles/tailwind.css -o ./src/Styles/tailwind.generated.css",
    "watch:css": "cross-env TAILWIND_MODE=watch yarn css --watch",
    "dev": "concurrently \"yarn watch:css\" \"vite\"",
    "prebuild": "cross-env NODE_ENV=production yarn css --minify"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^5.3.0",
    "valtio": "^1.1.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@tailwindcss/jit": "^0.1.18",
    "@testing-library/dom": "^8.10.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.13",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "@vitejs/plugin-react": "^1.0.0",
    "autoprefixer": "^10.3.1",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.9.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-tailwindcss": "^1.14.1",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.3.2",
    "vite": "^2.6.4",
    "vite-tsconfig-paths": "^3.3.17"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
